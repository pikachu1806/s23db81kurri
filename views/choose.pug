extends layout

block content
  h1= title

  - var image_names = ["LufthansaBoeing747.jpg","BoeingA747BA.jpg","AirbusA320.jpeg","NeoAirbusA320.jpg","Cessna172.jpg"];
  - for(i=0;i<3;i++) {
     - var randNum = Math.floor(Math.random()*5) ;
     img(src=`./images/${image_names[randNum]}` alt=`img${image_names[randNum]}` style='width:300px; height:300px;')

  -}